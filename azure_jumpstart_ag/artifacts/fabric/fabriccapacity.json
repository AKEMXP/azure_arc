{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.1",
  "parameters": {
      "fabricCapacityName": {
          "type": "string",
          "defaultValue": "{FABRIC_CAPACITY_NAME}"
      },
      "location": {
          "type": "string",
          "defaultValue": "{FABRIC_CAPACITY_LOCATION}"
      },
      "sku": {
          "type": "string",
          "defaultValue": "F2",
          "allowedValues": [
              "F2",
              "F4",
              "F8",
              "F16",
              "F32",
              "F64",
              "F128",
              "F256",
              "F512",
              "F1024",
              "F2048"
          ]
      },
      "fabricCapacityAdmin": {
          "type": "string",
          "defaultValue": "{FABRIC_CAPACITY_ADMIN}"
      },
      "tagValues": {
          "type": "object",
          "defaultValue": {}
      }
  },
  "variables": {},
  "resources": [
      {
          "apiVersion": "2022-07-01-preview",
          "name": "[parameters('fabricCapacityName')]",
          "location": "[parameters('location')]",
          "sku": {
              "name": "[parameters('sku')]",
              "tier": "Fabric"
          },
          "properties": {
              "administration": {
                  "members": [
                      "[parameters('fabricCapacityAdmin')]"
                  ]
              }
          },
          "type": "Microsoft.Fabric/capacities",
          "tags": "[parameters('tagValues')]"
      }
  ],
  "outputs": {}
}